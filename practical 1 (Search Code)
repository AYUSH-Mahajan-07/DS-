#include<stdio.h>
void main() {
    int a[10],i,found=0,n;
    printf("enter array elements:\n");
    for(i=0;i<10;i++){
        scanf("%d",&a[i]);
    }
    printf("enter the number you want to find in array ",&n);
    scanf("%d",&n);
    for(i=0;i<10;i++){
        if(a[i]==n){
            printf("The number is present in array");
            found++;
        }       
    }
    if (found==0)
        {
            printf("the number is not present");
        } 
    return 0;
}
